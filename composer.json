{
    "name": "tolkam/composer-custom-path",
    "description": "Allows installing composer packages into custom locations",
    "type": "composer-plugin",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Tolkam\\Composer\\CustomPath\\": "src"
        }
    },
    "extra": {
        "class": "Tolkam\\Composer\\CustomPath\\Plugin",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require": {
        "php": ">=7.4",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.3"
    }
}
