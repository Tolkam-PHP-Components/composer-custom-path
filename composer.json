{
    "name": "tolkam/composer-custom-path",
    "description": "Allows installing composer packages into custom locations",
    "type": "composer-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "Kanstantsin Kulesh",
            "email": "kk@tolkam.com",
            "homepage": "https://www.tolkam.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tolkam\\Composer\\CustomPath\\": "src"
        }
    },
    "extra": {
        "class": "Tolkam\\Composer\\CustomPath\\Plugin"
    },
    "require": {
        "php": ">=8.0",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    }
}
